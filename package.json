{
  "name": "interactive-stateful-process",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "rm -rf dist/* && tsc && yarn run doc",
    "doc": "rm -fr doc/* && typedoc --out doc src/index.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint src/*.ts tests/*.ts",
    "lint:fix": "eslint --fix src/*.ts tests/*.ts",
    "test:watch": "jest --watch",
    "test:ci": "yarn run lint && yarn test"
  },
  "repository": "git@github.com:wigy/react-interactive-stateful-process.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "knex": "^0.95.11",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/knex": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "better-docs": "^2.3.2",
    "eslint": "^7.10.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.25.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.22.6",
    "typescript": "^4.4.4"
  }
}
