    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-standard": "5.0.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.5"
