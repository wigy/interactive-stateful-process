{
  "name": "interactive-stateful-process",
  "version": "1.2.128",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "demo": "ts-node-dev example/server.ts",
    "dev": "./make.js --dev --minify",
    "build": "rm -rf dist/* && ./make.js --minify && tsc --emitDeclarationOnly && cp -r migrations dist/",
    "doc": "rm -fr doc/* && typedoc --out doc src/index.ts",
    "doc:watch": "typedoc --watch --out doc src/index.ts",
    "lint": "eslint src/*.ts tests/*.ts",
    "lint:fix": "eslint --fix src/*.ts tests/*.ts",
    "test:unit": "jest --forceExit --detectOpenHandles",
    "test:watch": "jest --watch --forceExit --detectOpenHandles"
  },
  "repository": "git@github.com:wigy/interactive-stateful-process.git",
  "author": "Tommi Ronkainen <tommi.ronkainen@gmail.com>",
  "license": "GPL-3.0-only",
  "dependencies": {
    "chardet": "^1.4.0",
    "clone": "^2.1.2",
    "csv-parse": "^4.16.3",
    "eslint": "8.19.0",
    "eslint-config-semistandard": "^16.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mocha": "^10.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-standard": "^5.0.0",
    "express": "4.17.2",
    "interactive-elements": "1.0.92",
    "knex": "2.0.0",
    "pg": "8.7.3",
    "ts-opaque": "^3.0.1"
  },
  "devDependencies": {
    "@types/clone": "^2.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/knex": "^0.16.1",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "cors": "^2.8.5",
    "esbuild": "^0.15.5",
    "jest": "^28.1.1",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "4.7.4"
  }
}
